    base:                               '8'
    alt:                                '('
    ctrl, shift, meta:                  none
}

key 9 {
    label, number:                      '9'
    base:                               '9'
    alt:                                ')'
    ctrl, shift, meta:                  none
}

key SPACE {
    label:                              ' '
    base:                               ' '
    alt:                                fallback PICTSYMBOLS
    ctrl, meta:                         fallback SEARCH
}

key ENTER {
    label:                              '\n'
    base:                               '\n'
    ctrl, alt, meta:                    none
}

key COMMA {
    label, number:                      ','
    base:                               ','
    shift:                              '<'
    ctrl, alt, meta:                    none
}

key PERIOD {
    label, number:                      '.'
    base:                               '.'
    shift, ctrl, alt, meta:             none
}

key MINUS {
    label, number:                      '-'
    base:                               '-'
    shift:                              '_'
    alt:                                '?'
    ctrl, meta:                         none
}


### Non-printing keys ###

key ESCAPE {
    base:                               fallback BACK
    alt, meta:                          fallback HOME
    ctrl:                               fallback MENU
}

key DPAD_DOWN {
    base:                               fallback DPAD_DOWN
    alt:                                fallback PAGE_DOWN
}

key DPAD_UP {
    base:                               fallback DPAD_UP
    alt:                                fallback PAGE_UP
}

key SWITCH_CHARSET {
    base:                               fallback SWITCH_CHARSET
    alt:                                fallback PICTSYMBOLS
}

