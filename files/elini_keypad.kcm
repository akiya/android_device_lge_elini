type FULL

key A {
    label:                              'A'
    base:                               'a'
    shift, capslock:                    'A'
    alt:                                '['
}

key B {
    label:                              'B'
    base:                               'b'
    shift, capslock:                    'B'
    ctrl, alt, meta:                    none
}

key C {
    label:                              'C'
    base:                               'c'
    shift, capslock:                    'C'
    ctrl, alt, meta:                    none
}

